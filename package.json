{
  "name": "express-test-01",
  "version": "0.0.1",
  "main": "src/index.js",
  "scripts": {
    "launch": "tsc && node build/index.js",
    "debug-brk": "node --nolazy --inspect-brk=9229 build/index.js",
    "nodeamon-debug": "nodemon --inspect --debug-port=9229 build/index.js",
    "nodeamon-debug-brk": "nodemon --inspect-brk --debug-port=9229 build/index.js",
    "watch-nodeamon-debug": "concurrently --kill-others \"npm run watch\" \"npm run nodeamon-debug\"",
    "watch-nodeamon-debug-brk": "concurrently --kill-others \"npm run watch\" \"npm run nodeamon-debug-brk\"",
    "build": "tsc",
    "watch": "tsc -w",
    "build-bundle": "tsc && webpack --config webpack.server.config.js"
  },
  "author": "Ilya Pikin",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.6.15",
    "@overnightjs/logger": "^1.1.10",
    "body-parser": "^1.19.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/node": "^13.13.1",
    "@types/validator": "^13.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.3",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
